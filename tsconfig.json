{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "target": "es2017",
    "module": "commonjs",
  },
  "types": [
    "naive-ui/volar",
    "@pinia/nuxt"
  ],
  "paths": {
    "#imports": [
      ".nuxt/imports"
    ],
  }
}